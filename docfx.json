{
  "build": {
    "content": [
      {
        "src":"api-ref",
        "files": [
          "**.json",
          "**/toc.md"
        ],
        "dest":"api"
      },
      {
        "src":"api-index",
        "files": [
          "index.md"
        ],
        "dest":"api"
      },
      {
        "files": [
          "*.md",
          "toc.yml"
        ],
        "exclude": [
          "**/obj/**",
          "rest/azure/**"
        ]
      },
      {
        "files": [
          "breadcrumb/toc.yml"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api-doc/**.md"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata": {
      "_mockServerUrl": "https://apiexproxy.azurewebsites.net/svc",
      "_mockServerAuthorization": "Bearer {token:https://graph.windows.net/}",
      "author": "openpublishbuild",
      "breadcrumb_path": "~/breadcrumb/toc.yml"
    },
    "fileMetadata": {},
    "template": [
      "docs.html"
    ],
    "dest": "rest/azure"
  }
}
