{
  "build": {
    "content": [
      {
        "src":"docs-ref-autogen",
        "files": [
          "**.yml"
        ],
        "exclude": [
          "toc.md"
        ],
        "dest":"api"
      },
      {
        "src":"docs-ref-autogen",
