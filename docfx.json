{
  "build": {
    "content": [
      {
        "src":"docs-ref-autogen",
        "files": [
          "**.json",
          "toc.md"
        ],
        "dest":"api"
      },
      {
        "src":"api-index",
        "files": [
          "index.md"
        ],
        "dest":"api"
      },
      {
        "src":"documentation",
        "files": [
          "**.md"
        ],
        "exclude": [
          "**/toc.md",
          "network/**.md",
          "mobile-engagement/**.md",
          "commerce/**.md"
        ],
        "dest":"api"
      },
      {
        "files": [
          "*.md",
          "toc.yml"
        ],
        "exclude": [
          "**/obj/**",
          "rest/**"
        ]
      },
      {
        "files": [
          "breadcrumb/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "src":"documentation",
        "files": [
          "**.png",
          "**.jpg"
        ],
        "dest":"api"
      }
    ],
    "overwrite": [
      {
        "files": [
          "api-doc/**.md"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata": {
      "_mockServerUrl": "https://apiexproxy.azurewebsites.net/svc",
      "_mockServerAuthorization": "Bearer {token:https://graph.windows.net/}",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "author": "erickson-doug",
      "ms.author": "douge",
      "manager": "douge",
      "ms.topic": "reference",
      "ms.devlang": "rest-api",
      "ms.date": "11/16/2016",
      "brand": "azure",
      "searchScope": ["Azure"]
    },
    "fileMetadata": {
      "ms.service": 
      {
        "docs-ref-autogen/analysisservices/**.json": "analysis-services",
        "docs-ref-autogen/apimanagement/**.json": "api-management",
        "docs-ref-autogen/appservice/**.json": "websites",
