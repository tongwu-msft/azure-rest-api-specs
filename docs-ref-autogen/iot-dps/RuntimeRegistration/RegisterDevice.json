{
  "swagger": "2.0",
  "info": {
    "version": "2017-08-31-preview",
    "title": "DeviceProvisioningService_DeviceRuntimeClient",
    "description": "API for device runtime operations with the Azure IotHub Device Provisioning Service"
  },
  "host": "global.azure-devices-provisioning.net",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "paths": {
    "/{idScope}/registrations/{registrationId}/register": {
      "put": {
        "tags": [
          "PUT"
        ],
        "summary": "Registers the devices.",
        "operationId": "RuntimeRegistration_RegisterDevice",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "registrationId",
            "in": "path",
            "description": "Registration ID.",
            "required": true,
            "type": "string"
          },
          {
            "name": "deviceRegistration",
            "in": "body",
            "description": "Device registration request.",
            "required": true,
            "schema": {
              "$ref": "#/definitions/DeviceRegistration"
            }
          },
          {
            "name": "forceRegistration",
            "in": "query",
            "description": "Force the device to re-register. Setting this option may assign the device to a different IotHub.",
            "required": false,
            "type": "boolean"
          },
          {
            "name": "idScope",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "202": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/RegistrationOperationStatus"
            }
          }
        },
        "x-internal-toc-name": "Register Device"
      }
    }
  },
  "definitions": {
